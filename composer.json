{
    "name": "sndatabase/mysql",
    "description": "MySQL Driver for the SN Database api",
    "type": "library",
    "keywords": ["database", "abstraction", "mysql"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samy Naamani",
            "email": "samy@namani.net"
        }
    ],
    "support": {
        "email": "samy@namani.net"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "PHP": ">=5.3",
        "sndatabase/core": "*",
        "sndatabase/pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.x",
        "phpunit/phpunit-skeleton-generator": "2.0.x"
    },
    "autoload": {
        "psr-4": {
            "SNDatabase\\MySQL\\": "src/"
        },
        "files": ["src/register.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "SNDatabase\\MySQL\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.3"
        }
    }
}
